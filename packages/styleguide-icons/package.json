    "@figma-export/transform-svg-with-svgo": "^4.5.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.27",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
