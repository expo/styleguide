{
  "name": "@expo/styleguide-icons",
  "version": "1.0.0-alpha.1",
  "description": "Expo's icons for use on the web.",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "check-env": "sh ./check-env.sh",
    "clean": "rimraf dist tmp",
    "export": "figma-export use-config figma.config.js",
    "bundle": "rollup --config",
    "build": "run-s check-env",
    "build-stub": "export STUB=true && run-s clean bundle",
    "build-icons": "run-s clean export bundle"
  },
  "author": "Jon Samp",
  "license": "UNLICENSED",
  "homepage": "https://github.com/expo/styleguide",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/styleguide.git",
    "directory": "packages/styleguide"
  },
  "bugs": {
    "url": "https://github.com/expo/styleguide/issues"
  },
  "dependencies": {
    "tailwind-merge": "^1.10.0"
  },
  "devDependencies": {
    "@figma-export/cli": "^4.5.0",
    "@figma-export/output-components-as-svgr": "^4.5.0",
    "@figma-export/transform-svg-with-svgo": "^4.5.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.27",
    "dotenv": "^16.0.3",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "rimraf": "^4.1.2",
    "rollup": "^3.17.2",
    "rollup-plugin-copy": "^3.4.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": ">= 16"
  },
  "eslintConfig": {
    "extends": [
      "universe/web",
      "universe/node"
    ]
  }
}
