{
  "name": "@expo/styleguide",
  "version": "4.1.2",
  "description": "Foundational styles for Expo interfaces.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "include-theme-css": "copyfiles --flat ./src/styles/expo-theme.css ./dist",
    "build:icons": "rimraf ./src/icons && svgr ../../common/svg --config-file ./svgr-icon-config.js --out-dir ./src/icons --index-template ./svgr-index-template.js",
    "build:logos": "rimraf ./src/logos && svgr ../../common/logos --config-file ./svgr-logo-config.js --out-dir ./src/logos --index-template ./svgr-index-template.js",
    "build": "yarn build:icons && yarn build:logos && yarn clean && yarn tsc && yarn include-theme-css",
    "dev": "yarn tsc -w"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/expo/styleguide",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/styleguide.git",
    "directory": "packages/styleguide"
  },
  "keywords": [
    "expo"
  ],
  "author": "Jon Samp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/expo/styleguide/issues"
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@types/node": "^15.3.1",
    "@types/react": "^17.0.6",
    "copyfiles": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "react": "*"
  },
  "gitHead": "94bfdec4905cfc8fa2c2540ed672219ec6949246"
}
